        "body": ["@typeparam TItem", "", "<table>", "\t<thead>", "\t\t<tr>@TableHeader</tr>", "\t</thead>", "\t<tbody>", "\t\t@foreach (var item in Items)", "\t\t{", "\t\t\t<tr>@RowTemplate(item)</tr>", "\t\t}", "\t</tbody>", "\t<tfoot>", "\t\t<tr>@TableFooter</tr>", "\t</tfoot>", "</table>", "", "@code {", "\t[Parameter] public RenderFragment TableHeader { get; set; }", "", "\t[Parameter] public RenderFragment<TItem> RowTemplate { get; set; }", "", "\t[Parameter] public RenderFragment TableFooter { get; set; }", "", "\t[Parameter] public IReadOnlyList<TItem> Items { get; set; }", "}"],
