        "prefix": "yFormGroupInputSelect"
    },
    "formGroupInputSelectForEach": {
        "body": ["<div class=\"form-group\">", "\t<label for=\"${1:idInputSelect}\">${2:LabelText}:</label>", "\t<div>", "\t\t<InputSelect id=\"${3:$1}\" class=\"form-control\" @bind-Value=\"${4:Model}.${5:Property}\" >$0", "\t\t\t<option disabled value=\"\">${6:Select...}</option>", "\t\t\t@foreach (var ${7:item} in ${8:Items})", "\t\t{", "\t\t\t\t<option value=\"${9:$7}.${10:Id}\">${11:$7}.${12:DisplayProperty}</option>", "\t\t\t}", "\t\t</InputSelect>", "\t\t<ValidationMessage For=\"@(() => ${13:$4}.${14:$5})\" />", "\t</div>", "</div>", "$0"],
        "prefix": "yFormGroupInputSelectForEach"
    },
